{
  "name": "Bethel_Server",
  "version": "1.0.0",
  "description": "Bethel WebServer NodeJS(Express) / GrapqhQL / TypeScript / Postgresql",
  "main": "index.js",
  "author": "Sean.Kim",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "graphql-tools": "^4.0.3",
    "graphql-yoga": "^1.16.7",
    "helmet": "^3.15.0",
    "logger": "^0.0.1",
    "morgan": "^1.9.1",
    "pg": "^7.6.1",
    "typeorm": "^0.2.9"
  },
  "scripts": {
    "predev": "yarn run types",
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts, graphql",
